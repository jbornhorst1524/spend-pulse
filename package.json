{
  "name": "spend-pulse",
  "version": "0.1.0",
  "description": "CLI tool for proactive spending alerts via Plaid",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "spend-pulse": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "SKILL.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "lint": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "spending",
    "plaid",
    "budget",
    "cli",
    "alerts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbornhorst1524/spend-pulse.git"
  },
  "homepage": "https://github.com/jbornhorst1524/spend-pulse#readme",
  "bugs": {
    "url": "https://github.com/jbornhorst1524/spend-pulse/issues"
  },
  "author": "Jason Bornhorst",
  "license": "MIT",
  "dependencies": {
    "commander": "^12.0.0",
    "js-yaml": "^4.1.0",
    "keytar": "^7.9.0",
    "open": "^11.0.0",
    "plaid": "^41.1.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/keytar": "^4.4.0",
    "@types/node": "^20.11.0",
    "@types/open": "^6.1.0",
    "@types/prompts": "^2.4.9",
    "@vitest/coverage-v8": "^3.2.4",
    "typescript": "^5.3.0",
    "vitest": "^3.2.4"
  }
}
